# Configuración de la base de datos
DATABASE_URL=postgresql://user:password@localhost:5432/credit_agents_db

# Configuración de seguridad
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Configuración del servidor
PORT=8000
HOST=0.0.0.0
DEBUG=True

# Configuración de OpenAI para los agentes
OPENAI_API_KEY=your-openai-api-key

# Configuración de GPT-OSS-20B
GPT_OSS_MODEL_URL=http://localhost:8080
USE_OPENAI_FALLBACK=true
DEFAULT_LLM_MODEL=gpt-oss-20b

# Configuración de Twilio para WhatsApp y SMS
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number
TWILIO_WHATSAPP_NUMBER=your-twilio-whatsapp-number

# Configuración de SendGrid para Email
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=your-sendgrid-from-email

# Configuración de la API externa de créditos
CREDIT_API_URL=https://api.example.com/credit
CREDIT_API_KEY=your-credit-api-key