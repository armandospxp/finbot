# Configuración de PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=credit_app

# Configuración del administrador
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin123

# Configuración de OpenAI (fallback)
OPENAI_API_KEY=your-openai-api-key

# Configuración de GPT-OSS-20B
USE_OPENAI_FALLBACK=true
DEFAULT_LLM_MODEL=gpt-oss-20b

# Configuración de Twilio para WhatsApp y SMS
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number
TWILIO_WHATSAPP_NUMBER=your-twilio-whatsapp-number

# Configuración de SendGrid para Email
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=your-sendgrid-from-email

# Configuración de la API externa de créditos
CREDIT_API_URL=https://api.example.com/credit
CREDIT_API_KEY=your-credit-api-key